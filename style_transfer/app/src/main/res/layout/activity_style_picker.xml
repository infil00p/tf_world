<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B7000000"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/GrandCanyonButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="11dp"
        app:srcCompat="@mipmap/grand_canyon"
        app:layout_constraintRight_toLeftOf="@id/LassenButton"
        tools:layout_editor_absoluteY="42dp" />

    <ImageButton
        android:id="@+id/LassenButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/GrandCanyonButton"
        app:srcCompat="@mipmap/lassen"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="44dp" />

    <ImageButton
        android:id="@+id/ZionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/GrandCanyonButton"
        app:srcCompat="@mipmap/zion"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="350dp" />


</androidx.constraintlayout.widget.ConstraintLayout>